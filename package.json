{
  "author": "Thierry Wong",
  "license": "AGPL-3.0-only",
  "name": "@gnowth/react",
  "private": true,
  "repository": "https://github.com/gnowth/react.git",
  "version": "0.2.0",
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": "10.15.0",
    "npm": "6.4.1"
  },
  "jestWebpackResolver": {
    "silent": true,
    "webpackConfig": "./configs/webpack.config.js"
  },
  "scripts": {
    "analyze": "webpack --mode production --progress --config configs/webpack.config.js",
    "build": "webpack --mode production --progress --config configs/webpack.config.js",
    "clean": "lerna clean",
    "coveralls": "jest --config configs/jest.config.js --coverage --collectCoverageFrom={src,packages}/**/*.{js,jsx} --collectCoverageFrom=!{src,packages}/**/*stories.{js,jsx} && cat ./coverage/lcov.info | coveralls",
    "deploy": "./scripts/deploy-ghpages.sh",
    "fetch": "git fetch --prune",
    "git-view": "git log --all --graph --decorate --pretty=oneline --abbrev-commit",
    "lerna": "lerna",
    "lint": "npm run lint:css && npm run lint:js && npm run packages-lint",
    "lint:css": "stylelint 'src/**/*.{js,jsx}' --config configs/stylelint.config.js",
    "lint:js": "eslint -c configs/eslint.config.js --ext .js --ext .jsx src",
    "packages-build": "lerna run build",
    "packages-clean": "lerna run clean",
    "packages-link-main:dist": "lerna run link-main:dist",
    "packages-link-main:src": "lerna run link-main:src",
    "packages-lint": "lerna run lint",
    "packages-publish": "./scripts/publish.sh",
    "serve-dist": "browser-sync start --server dist",
    "start": "webpack-dev-server --mode development --progress --inline --hot --content-base dist --config configs/webpack.config.js",
    "storybook": "start-storybook -c .storybook -p 6006",
    "storybook:build": "build-storybook -c .storybook -o dist/style-guide",
    "test": "jest --config configs/jest.config.js",
    "yarn": "yarn"
  },
  "dependencies": {
    "@entity/core": "0.2.0",
    "@entity/duck": "0.2.0",
    "@entity/duck-namespace-drf": "0.2.0",
    "@entity/duck-namespace-screen": "0.2.0",
    "@entity/duck-query": "0.2.0",
    "@entity/form": "0.2.0",
    "@entity/view": "0.2.0",
    "@entity/widget": "0.2.0",
    "@gnowth/app": "0.2.0",
    "@gnowth/higher-order-component": "0.2.0",
    "@gnowth/prop-types-duck": "0.2.0",
    "@gnowth/prop-types-entity": "0.2.0",
    "@gnowth/prop-types-plus": "0.2.0",
    "@gnowth/style": "0.2.0",
    "@gnowth/ui": "0.2.0",
    "axios": "0.18.0",
    "connected-react-router": "6.2.1",
    "coveralls": "3.0.2",
    "font-awesome": "4.7.0",
    "github-markdown-css": "2.10.0",
    "history": "4.7.2",
    "immutable": "4.0.0-rc.12",
    "intl": "1.2.5",
    "lodash": "4.17.11",
    "material-design-icons": "3.0.1",
    "moment": "2.23.0",
    "normalize.css": "8.0.1",
    "prop-types": "15.6.2",
    "prop-types-exact": "1.2.0",
    "react": "16.7.0",
    "react-datepicker": "2.0.0",
    "react-dom": "16.7.0",
    "react-immutable-proptypes": "2.1.0",
    "react-intl": "2.7.2",
    "react-markdown": "4.0.6",
    "react-redux": "6.0.0",
    "react-router-dom": "4.3.1",
    "react-router-prop-types": "1.0.4",
    "redux": "4.0.1",
    "redux-immutable": "4.0.0",
    "styled-components": "4.1.3",
    "xdomain": "0.7.3"
  },
