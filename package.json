    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.5",
    "babel-plugin-react-flow-props-to-prop-types": "0.15.0",
    "babel-plugin-styled-components": "1.10.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.21",
    "browser-sync": "2.26.3",
    "browser-sync-webpack-plugin": "2.2.2",
