    "webpack": "4.28.4",
    "webpack-bundle-analyzer": "3.0.3",
    "webpack-cli": "3.2.1",
    "webpack-dev-server": "3.1.14",
    "webpack-s3-plugin": "1.0.3",
    "workbox-webpack-plugin": "^3.6.3",
    "yarn": "1.13.0"
  }
}
