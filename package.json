{
  "author": "Thierry Wong",
  "license": "GPL-3.0-or-later",
  "name": "@gnowth/react",
  "private": true,
  "repository": "https://github.com/gnowth/react.git",
  "version": "0.0.1",
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": "10.11.0",
    "npm": "6.4.1"
  },
  "scripts": {
    "build": "npm run build:src",
    "build:src": "webpack --mode production --progress --config configs/webpack.config.js",
    "build:storybook": "build-storybook -c configs/storybook -o dist/styleguide",
    "deploy": "npm run build && npm run storybook:build",
    "lint": "npm run lint:src && npm run packages-lint",
    "lint:src": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint '{lib,src}/**/*.{js,jsx}' --config configs/stylelint.config.js",
    "lint:js": "eslint -c configs/eslint.config.js --ext .js --ext .jsx src",
    "packages-build": "lerna run build",
    "packages-clean": "lerna run clean",
    "packages-link-main:dist": "lerna run link-main:dist",
    "packages-link-main:src": "lerna run link-main:src",
    "packages-lint": "lerna run lint",
    "packages-test": "lerna run test",
    "packages-publish": "./scripts/publish",
    "packages-version-update": "lerna publish --skip-git --skip-npm --force-publish '*'",
    "start": "webpack-dev-server --mode development --progress --inline --hot --content-base dist --config configs/webpack.config.js",
    "test": "jest --config configs/jest.config.js",
    "serve-dist": "browser-sync start --server dist",
    "storybook": "start-storybook -c configs/storybook -p 6006",
    "yarn": "yarn"
  },
  "dependencies": {
    "@entity/core": "0.0.1",
    "@entity/duck": "0.0.1",
    "@entity/duck-namespace-drf": "0.0.1",
    "@entity/duck-namespace-screen": "0.0.1",
    "@entity/duck-query": "0.0.1",
    "@entity/form": "0.0.1",
    "@entity/view": "0.0.1",
    "@entity/widget": "0.0.1",
    "@gnowth/app": "0.0.1",
    "@gnowth/higher-order-component": "0.0.1",
    "@gnowth/prop-types-duck": "0.0.1",
    "@gnowth/prop-types-entity": "0.0.1",
    "@gnowth/prop-types-plus": "0.0.1",
    "@gnowth/ui": "0.0.1",
    "axios": "0.18.0",
    "connected-react-router": "4.5.0",
    "history": "4.7.2",
    "immutable": "4.0.0-rc.9",
    "intl": "1.2.5",
    "lodash": "4.17.11",
    "moment": "2.22.2",
    "normalize.css": "8.0.0",
    "polished": "2.2.0",
    "prop-types": "15.6.2",
    "prop-types-exact": "1.2.0",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-immutable-proptypes": "2.1.0",
    "react-intl": "2.7.0",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "redux": "4.0.0",
    "redux-immutable": "4.0.0",
    "styled-components": "3.4.9",
    "xdomain": "0.7.3"
  },
  "devDependencies": {
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "7.0.0",
    "@babel/plugin-transform-react-jsx": "7.0.0",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "@storybook/addon-actions": "3.4.11",
    "@storybook/addon-links": "3.4.11",
    "@storybook/react": "3.4.11",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "babel-plugin-react-flow-props-to-prop-types": "0.15.0",
    "babel-plugin-styled-components": "1.8.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.18",
    "browser-sync": "2.24.7",
    "browser-sync-webpack-plugin": "2.2.2",
    "core-js": "2.5.7",
    "css-loader": "1.0.0",
    "enzyme": "3.6.0",
    "enzyme-adapter-react-16": "1.5.0",
    "eslint": "5.6.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-import-resolver-webpack": "0.10.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.1",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "2.0.0",
    "html-webpack-plugin": "3.2.0",
    "jest": "23.6.0",
    "json": "9.0.6",
    "lerna": "3.4.0",
    "raw-loader": "1.0.0-beta.0",
    "react-test-renderer": "16.5.2",
    "rimraf": "2.6.2",
    "rollup": "0.66.2",
    "rollup-plugin-babel": "4.0.3",
    "rollup-plugin-commonjs": "9.1.8",
    "rollup-plugin-copy": "0.2.3",
    "rollup-plugin-node-resolve": "3.4.0",
    "rollup-plugin-postcss": "1.6.2",
    "rollup-plugin-replace": "2.0.0",
    "rollup-plugin-terser": "3.0.0",
    "style-loader": "0.23.0",
    "stylelint": "9.6.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.4.0",
    "webpack": "4.20.2",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.9",
    "webpack-s3-plugin": "1.0.2",
    "yarn": "1.10.1"
  }
}
