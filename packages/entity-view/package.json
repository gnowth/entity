{
  "author": "Thierry Wong",
  "description": "entity-view",
  "license": "AGPL-3.0-only",
  "main": "src/index.js",
  "name": "@entity/view",
  "private": false,
  "repository": "https://github.com/gnowth/react.git",
  "sideEffects": false,
  "version": "0.4.0",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rollup -c=../../configs/rollup.config.js",
    "destroy": "rimraf dist",
    "link-main:dist": "json -I -f package.json -e 'this.main=\"dist/index.js\"'",
    "link-main:src": "json -I -f package.json -e 'this.main=\"src/index.js\"'",
    "postbuild": "npm run link-main:dist",
    "postdestroy": "npm run link-main:src",
    "prebuild": "npm run destroy"
  },
  "dependencies": {
    "@gnowth/default": "0.4.0",
    "@gnowth/higher-order-component": "0.4.0",
    "@gnowth/prop-types-entity": "0.4.0",
    "@gnowth/prop-types-plus": "0.4.0",
    "@gnowth/style": "0.4.0",
    "@gnowth/ui": "0.4.0",
    "prop-types": "15.7.2",
    "prop-types-exact": "1.2.0",
    "react-immutable-proptypes": "2.1.0"
  },
  "devDependencies": {
    "@entity/core": "0.4.0",
    "@entity/duck-namespace-screen": "0.4.0",
    "@entity/duck-query": "0.4.0",
    "react-intl": "2.8.0",
    "react-redux": "6.0.1",
    "redux-mock-store": "1.5.3"
  },
  "peerDependencies": {
    "immutable": "4.0.0-rc.12",
    "react": "16.8.3",
    "react-router-dom": "4.3.1",
    "react-test-renderer": "16.8.3",
    "styled-components": "4.1.3"
  }
}
