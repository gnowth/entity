{
  "author": "Thierry Wong",
  "description": "entity-visualisation-pie-chart",
  "license": "AGPL-3.0-only",
  "main": "src/index.js",
  "module": "src/index.js",
  "name": "@entity/visualisation-pie-chart",
  "private": false,
  "repository": "https://github.com/gnowth/entity.git",
  "sideEffects": false,
  "version": "0.5.1",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rollup -c=../../configs/rollup.config.js",
    "destroy": "rimraf dist",
    "link-index:dist": "json -I -f package.json -e 'this.main=\"dist/index.js\"; this.module=\"dist/index.js\"'",
    "link-index:src": "json -I -f package.json -e 'this.main=\"src/index.js\"; this.module=\"src/index.js\"'",
    "postdestroy": "npm run link-index:src",
    "prebuild": "npm run destroy && npm run link-index:dist"
  },
  "dependencies": {
    "@entity/visualisation": "0.5.1",
    "@gnowth/prop-types-entity": "0.5.1",
    "d3": "5.9.2",
    "immutable": "3.8.2",
    "lodash": "4.17.11",
    "prop-types": "15.7.2",
    "react-immutable-proptypes": "2.1.0"
  },
  "devDependencies": {
    "react": "16.8.4"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  }
}
