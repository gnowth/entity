{
  "author": "Thierry Wong",
  "description": "entity-widget",
  "license": "GPL-3.0-or-later",
  "main": "src/index.js",
  "name": "@entity/widget",
  "private": true,
  "repository": "https://github.com/gnowth/react.git",
  "version": "0.0.1",
  "scripts": {
    "build": "rollup -c=../../configs/rollup.config.js",
    "clean": "rimraf dist",
    "link-main:dist": "json -I -f package.json -e 'this.main=\"dist/index.js\"'",
    "link-main:src": "json -I -f package.json -e 'this.main=\"src/index.js\"'",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint 'src/**/*.{js,jsx}' --config ../../configs/stylelint.config.js",
    "lint:js": "eslint -c ../../configs/eslint.config.js --ext .js --ext .jsx src",
    "prebuild": "npm run clean"
  },
  "dependencies": {
    "@gnowth/prop-types-entity": "0.0.1",
    "@gnowth/ui": "0.0.1",
    "immutable": "4.0.0-rc.9",
    "lodash": "4.17.11",
    "prop-types": "15.6.2",
    "react-immutable-proptypes": "2.1.0",
    "react-select": "2.0.0",
    "reselect": "3.0.1",
    "styled-components": "3.4.9"
  },
  "peerDependencies": {
    "react": "16.5.2",
    "react-router-dom": "4.3.1"
  }
}
