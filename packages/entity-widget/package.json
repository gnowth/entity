{
  "author": "Thierry Wong",
  "description": "entity-widget",
  "license": "AGPL-3.0-only",
  "main": "src/index.js",
  "module": "src/index.js",
  "name": "@entity/widget",
  "private": false,
  "repository": "https://github.com/gnowth/entity.git",
  "sideEffects": false,
  "version": "0.5.1",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rollup -c=../../configs/rollup.config.js",
    "destroy": "rimraf dist",
    "link-main:dist": "json -I -f package.json -e 'this.main=\"dist/index.js\"'",
    "link-main:src": "json -I -f package.json -e 'this.main=\"src/index.js\"'",
    "link-module:dist": "json -I -f package.json -e 'this.module=\"dist/index.js\"'",
    "link-module:src": "json -I -f package.json -e 'this.module=\"src/index.js\"'",
    "postdestroy": "npm run link-main:src && npm run link-module:src",
    "prebuild": "npm run destroy && npm run link-main:dist && npm run link-module:dist"
  },
  "dependencies": {
    "@entity/core": "0.5.1",
    "@gnowth/default": "0.5.1",
    "@gnowth/higher-order-component": "0.5.1",
    "@gnowth/prop-types-entity": "0.5.1",
    "@gnowth/prop-types-plus": "0.5.1",
    "@gnowth/theme": "0.5.1",
    "@gnowth/ui": "0.5.1",
    "immutable": "4.0.0-rc.12",
    "prop-types": "15.7.2",
    "react-immutable-proptypes": "2.1.0",
    "react-select": "2.4.2",
    "reselect": "4.0.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "4.1.13",
    "@storybook/addon-knobs": "4.1.13",
    "jest-styled-components": "6.3.1",
    "lodash": "4.17.11",
    "moment": "2.24.0",
    "react": "16.8.4",
    "react-datepicker": "2.2.0",
    "react-dom": "16.8.4",
    "react-intl": "2.8.0",
    "react-router-dom": "4.3.1",
    "react-test-renderer": "16.8.4",
    "styled-components": "4.1.3"
  },
  "peerDependencies": {
    "@storybook/addon-actions": ">=4.0.0",
    "@storybook/addon-knobs": ">=4.0.0",
    "lodash": ">=4.12.0",
    "moment": ">=2.0.0",
    "react": ">=16.8.0",
    "react-datepicker": ">=2.0.0",
    "react-dom": ">=16.8.0",
    "react-intl": ">=2.0.0",
    "react-router-dom": ">=4.0.0",
    "react-test-renderer": ">=16.8.0",
    "styled-components": ">=4.0.0"
  }
}
