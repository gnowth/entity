{
  "author": "Thierry Wong",
  "description": "ui",
  "license": "AGPL-3.0-only",
  "main": "src/index.js",
  "name": "@gnowth/ui",
  "private": false,
  "repository": "https://github.com/gnowth/react.git",
  "version": "0.2.0",
  "scripts": {
    "build": "rollup -c=../../configs/rollup.config.js",
    "clean": "rimraf dist",
    "link-main:dist": "json -I -f package.json -e 'this.main=\"dist/index.js\"'",
    "link-main:src": "json -I -f package.json -e 'this.main=\"src/index.js\"'",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint 'src/**/*.{js,jsx}' --config ../../configs/stylelint.config.js",
    "lint:js": "eslint -c ../../configs/eslint.config.js --ext .js --ext .jsx src",
    "postbuild": "npm run link-main:dist",
    "postclean": "npm run link-main:src",
    "prebuild": "npm run clean"
  },
  "dependencies": {
    "@gnowth/higher-order-component": "0.2.0",
    "@gnowth/prop-types-plus": "0.2.0",
    "@gnowth/style": "0.2.0",
    "classnames": "2.2.6",
    "immutable": "4.0.0-rc.12",
    "lodash": "4.17.11",
    "prop-types": "15.6.2",
    "react-immutable-proptypes": "2.1.0",
    "react-intl": "2.7.2"
  },
  "peerDependencies": {
    "@storybook/addon-a11y": "4.1.6",
    "@storybook/addon-actions": "4.1.6",
    "@storybook/addon-centered": "4.1.6",
    "@storybook/addon-knobs": "4.1.6",
    "@storybook/react": "4.1.6",
    "react": "16.7.0",
    "react-router-dom": "4.3.1",
    "styled-components": "4.1.3"
  }
}
