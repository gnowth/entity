  "sideEffects": false,
  "version": "0.5.1",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rollup -c=../../configs/rollup.config.js",
    "destroy": "rimraf dist",
    "link-index:dist": "json -I -f package.json -e 'this.main=\"dist/index.cjs.js\"; this.module=\"dist/index.esm.js\"'",
    "link-index:src": "json -I -f package.json -e 'this.main=\"src/index.js\"; this.module=\"src/index.js\"'",
    "postdestroy": "npm run link-index:src",
    "prebuild": "npm run link-index:dist"
  },
  "dependencies": {
    "@gnowth/default": "0.5.1",
    "@gnowth/higher-order-component": "0.5.1",
    "@gnowth/prop-types-plus": "0.5.1",
    "@gnowth/theme": "0.5.1",
    "classnames": "2.2.6",
    "idx": "2.5.5",
    "immutable": "3.8.2",
    "lodash": "4.17.11",
    "prop-types": "15.7.2",
    "prop-types-exact": "1.2.0",
    "react-immutable-proptypes": "2.1.0",
    "react-intl": "2.8.0",
    "react-onclickoutside": "6.8.0",
    "react-router-dom": "5.0.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "5.0.10",
    "@storybook/addon-knobs": "5.0.10",
    "jest-styled-components": "6.3.1",
