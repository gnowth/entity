{
  "author": "Thierry Wong",
  "description": "app",
  "license": "AGPL-3.0-only",
  "main": "src/index.js",
  "name": "@gnowth/app",
  "private": false,
  "repository": "https://github.com/gnowth/entity.git",
  "sideEffects": false,
  "version": "0.5.0",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rollup -c=../../configs/rollup.config.js",
    "destroy": "rimraf dist",
    "link-main:dist": "json -I -f package.json -e 'this.main=\"dist/index.js\"'",
    "link-main:src": "json -I -f package.json -e 'this.main=\"src/index.js\"'",
    "postbuild": "npm run link-main:dist",
    "postdestroy": "npm run link-main:src",
    "prebuild": "npm run destroy"
  },
  "dependencies": {
    "@gnowth/default": "0.5.0",
    "@gnowth/prop-types-entity": "0.5.0",
    "@gnowth/prop-types-plus": "0.5.0",
    "@gnowth/theme": "0.5.0",
    "@private/use-redux": "0.5.0",
    "idx": "2.5.5",
    "prop-types": "15.7.2",
    "prop-types-exact": "1.2.0"
  },
  "devDependencies": {
    "lodash": "4.17.11",
    "react": "16.8.4",
    "react-router-dom": "4.3.1",
    "styled-components": "4.1.3"
  },
  "peerDependencies": {
    "lodash": ">=4.12.0",
    "react": ">=16.8.0",
    "react-router-dom": ">=4.0.0",
    "styled-components": ">=4.0.0"
  }
}
