{
  "author": "Thierry Wong",
  "description": "entity-duck-query",
  "license": "AGPL-3.0-only",
  "main": "src/index.js",
  "module": "src/index.js",
  "name": "@entity/duck-query",
  "private": false,
  "repository": "https://github.com/gnowth/entity.git",
  "sideEffects": false,
  "version": "0.5.1",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rollup -c=../../configs/rollup.config.js",
    "destroy": "rimraf dist",
    "link-index:dist": "json -I -f package.json -e 'this.main=\"dist/index.cjs.js\"; this.module=\"dist/index.esm.js\"'",
    "link-index:src": "json -I -f package.json -e 'this.main=\"src/index.js\"; this.module=\"src/index.js\"'",
    "postdestroy": "npm run link-index:src",
    "prebuild": "npm run link-index:dist"
  },
  "dependencies": {
    "@gnowth/default": "0.5.1",
    "@gnowth/prop-types-plus": "0.5.1",
    "@private/use-redux": "0.5.1",
    "immutable": "3.8.2",
    "lodash": "4.17.11",
    "prop-types": "15.7.2",
    "prop-types-exact": "1.2.0"
  },
  "devDependencies": {
    "react": "16.8.6"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  }
}
