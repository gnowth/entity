{
  "author": "Thierry Wong",
  "description": "entity-duck",
  "license": "GPL-3.0-or-later",
  "main": "src/index.js",
  "name": "@entity/duck",
  "private": false,
  "repository": "https://github.com/gnowth/react.git",
  "version": "0.1.0",
  "scripts": {
    "build": "rollup -c=../../configs/rollup.config.js",
    "clean": "rimraf dist",
    "link-main:dist": "json -I -f package.json -e 'this.main=\"dist/index.js\"'",
    "link-main:src": "json -I -f package.json -e 'this.main=\"src/index.js\"'",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint 'src/**/*.{js,jsx}' --config ../../configs/stylelint.config.js",
    "lint:js": "eslint -c ../../configs/eslint.config.js --ext .js --ext .jsx src",
    "prebuild": "npm run clean"
  },
  "dependencies": {
    "@entity/core": "^0.1.0",
    "lodash": "4.17.11",
    "redux-actions": "2.6.4"
  }
}
