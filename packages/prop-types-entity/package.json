{
  "author": "Thierry Wong",
  "description": "prop-types-entity",
  "license": "AGPL-3.0-only",
  "main": "src/index.js",
  "name": "@gnowth/prop-types-entity",
  "private": false,
  "repository": "https://github.com/gnowth/entity.git",
  "sideEffects": false,
  "version": "0.5.0",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rollup -c=../../configs/rollup.config.js",
    "destroy": "rimraf dist",
    "link-main:dist": "json -I -f package.json -e 'this.main=\"dist/index.js\"'",
    "link-main:src": "json -I -f package.json -e 'this.main=\"src/index.js\"'",
    "postbuild": "npm run link-main:dist",
    "postdestroy": "npm run link-main:src",
    "prebuild": "npm run destroy"
  },
  "dependencies": {
    "@entity/core": "0.5.0",
    "@gnowth/prop-types-plus": "0.5.0",
    "prop-types": "15.7.2"
  }
}
